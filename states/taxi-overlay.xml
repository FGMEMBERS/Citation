<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<PropertyList>

  <name type="string" n="1">taxi</name>

  <overlay>
    <autopilot>
      <settings>
        <icao-aircraft-category type="string">C</icao-aircraft-category>
        <bank-limit type="double">27</bank-limit>
        <vertical-speed-fpm type="double">0</vertical-speed-fpm>
        <target-altitude-ft type="double">10000</target-altitude-ft>
        <gs1-arm type="bool">false</gs1-arm>
        <alt-arm type="bool">false</alt-arm>
        <target-speed-kt alias="/instrumentation[0]/airspeed-indicator[0]/index-marker[0]"/>
        <gps-driving-true-heading type="bool">false</gps-driving-true-heading>
        <true-heading-deg type="double">0</true-heading-deg>
        <target-pitch-deg type="double">0</target-pitch-deg>
        <heading-bug-deg type="double">0</heading-bug-deg>
        <target-aoa-deg type="double">0</target-aoa-deg>
        <target-agl-ft type="double">0</target-agl-ft>
      </settings>

      <locks>
        <yaw-damper type="bool">false</yaw-damper>
        <passive-mode type="bool">false</passive-mode>
        <lowbank type="bool">false</lowbank>
        <autothrottle type="bool">false</autothrottle>
        <heading type="string"></heading>
        <altitude type="string"></altitude>
        <speed type="string"></speed>
      </locks>
    </autopilot>


    <consumables>
      <fuel>
        <fuel_overlay type="bool">true</fuel_overlay>
        <fuel_overlay_0 type="double">255</fuel_overlay_0>
        <fuel_overlay_1 type="double">255</fuel_overlay_1>
      </fuel>
    </consumables>


    <controls>

      <flight>
        <flaps type="double">0</flaps>
        <flaps-serviceable type="bool">true</flaps-serviceable>
        <speedbrake-switch type="bool">false</speedbrake-switch>
      </flight>

      <engines>
        <engine n="0">
          <starter type="bool">false</starter>
          <fuel-pump type="bool">false</fuel-pump>
          <cutoff type="bool">false</cutoff>
          <feed_tank type="int">-1</feed_tank>
          <WEP type="bool">false</WEP>
          <ignition type="int">1</ignition>
          <reverser type="bool">false</reverser>
          <throttle type="double">0</throttle>
        </engine>

        <engine n="1">
          <starter type="bool">false</starter>
          <fuel-pump type="bool">false</fuel-pump>
          <cutoff type="bool">true</cutoff>
          <feed_tank type="int">-1</feed_tank>
          <WEP type="bool">false</WEP>
          <ignition type="int">1</ignition>
          <reverser type="bool">false</reverser>
          <throttle type="double">0</throttle>
        </engine>
      </engines>

      <gear>
        <brake-parking type="double">1</brake-parking>
        <gear-down type="bool">true</gear-down>
      </gear>

      <electric>
        <avionics-switch type="bool">true</avionics-switch>
        <battery-bus-switch type="int">1</battery-bus-switch>
        <voltage-sel type="int">0</voltage-sel>
        <system-test type="int">10</system-test>
        <fuel-sel type="int">0</fuel-sel>
        <engine>
          <generator type="bool">true</generator>
        </engine>
        <engine n="1">
          <generator type="bool">true</generator>
        </engine>
      </electric>

      <fuel>
        <tank>
          <fuel_selector type="bool">false</fuel_selector>
          <to_engine type="int">0</to_engine>
          <to_tank type="int">0</to_tank>
          <boost-pump type="bool">false</boost-pump>
          <boost-pump n="1" type="bool">false</boost-pump>
          <boost-pump-switch type="double">0</boost-pump-switch>
        </tank>

        <tank n="1">
          <fuel_selector type="bool">false</fuel_selector>
          <to_engine type="int">0</to_engine>
          <to_tank type="int">0</to_tank>
          <boost-pump type="bool">false</boost-pump>
          <boost-pump n="1" type="bool">false</boost-pump>
          <boost-pump-switch type="double">0</boost-pump-switch>
        </tank>
      </fuel>

      <lighting>
        <beacon-switch type="bool">true</beacon-switch>
        <landing-light-switch n="0" type="bool">true</landing-light-switch>
        <landing-light-switch n="1" type="bool">true</landing-light-switch>
        <nav-lights-switch type="bool">true</nav-lights-switch>
        <strobe-switch type="bool">true</strobe-switch>

        <panel-lights-switch type="bool">true</panel-lights-switch>
        <instrument-lights-norm type="double">0.5</instrument-lights-norm>
        <left-panel-norm type="double">0.2</left-panel-norm>
        <center-panel-norm type="double">0.2</center-panel-norm>
        <right-panel-norm type="double">0.2</right-panel-norm>
      </lighting>
    </controls>


    <engines>
      <engine>
        <reverser-position type="double">0</reverser-position>
        <started type="bool">true</started>
        <running type="bool">true</running>
      </engine>

      <engine n="1">
        <reverser-position type="double">0</reverser-position>
        <started type="bool">true</started>
        <running type="bool">true</running>
      </engine>
    </engines>

    <gear>
      <gear>
        <position-norm type="float">1</position-norm>
      </gear>
      <gear n="1">
        <position-norm type="float">1</position-norm>
      </gear>
      <gear n="2">
        <position-norm type="float">1</position-norm>
      </gear>
    </gear>

  </overlay>

</PropertyList>
