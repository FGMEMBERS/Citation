<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>

  <path>circuit-breakers.ac</path>

<!--
########################
####  EMERGENCY BUS ####
########################
  -->

  <animation>
    <type>translate</type>
    <object-name>cb.Light.ELPanel</object-name>
    <property>systems/electrical/circuit-breakers/bus-emer/cb-light-panel-el</property>
    <factor>-0.004</factor>
    <offset-m>0.004</offset-m>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>cb.Light.ELPanel</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>systems/electrical/circuit-breakers/bus-emer/cb-light-panel-el</property>
      </binding>
    </action>
  </animation>

