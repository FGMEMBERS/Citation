  <animation>
    <type>pick</type>
    <object-name>GearLever</object-name>
    <condition>
      <and>
        <property>controls/electric/circuit-breakers/bus-left/cb-gear-ctl</property>
	<greater-than>
	  <property>systems/electrical/outputs/bus/left</property>
	  <value>20</value>
	</greater-than>
      </and>
    </condition>
    <action>
      <button>0</button>
      <binding>
        <command>nasal</command>
        <script>controls.gearDown(1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>controls.gearDown(0)</script>
        </binding>
      </mod-up>
    </action>
  </animation>

  <animation>
    <type>pick</type>
