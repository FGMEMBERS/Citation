  <!--   Controls   -->

  <animation>
    <type>select</type>
    <object-name>Yoke</object-name>
    <object-name>Yoke1</object-name>
    <object-name>Yoke2</object-name>
    <condition>
      <property>sim/yokes-visible</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>nose.lamp</object-name>
    <object-name>LH.lamp</object-name>
    <object-name>RH.lamp</object-name>
    <condition>
      <greater-than>
        <property>systems/electrical/ac-volts</property>
        <value>90</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>nose.lamp</object-name>
    <condition>
      <equals>
        <property>gear/gear/position-norm</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LH.lamp</object-name>
    <condition>
      <equals>
        <property>gear/gear[1]/position-norm</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>RH.lamp</object-name>
    <condition>
      <equals>
        <property>gear/gear[2]/position-norm</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Gear.unlocked</object-name>
    <condition>
      <and>
        <less-than>
          <property>gear/gear/position-norm</property>
          <value>1</value>
        </less-than>
        <greater-than>
          <property>gear/gear/position-norm</property>
          <value>0</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Rvsr.arm</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/reverser-norm</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Rvsr.unlock</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/reverser-norm</property>
        <value>0.3</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Rvsr.deploy</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/reverser-norm</property>
        <value>0.9</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>GearLever</object-name>
    <property>controls/gear/gear-down</property>
    <factor>25</factor>
    <center>
      <x-m>2.4</x-m>
      <y-m>0</y-m>
      <z-m>0.360</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ElevTrim.Indicator</object-name>
    <property>controls/flight/elevator-trim</property>
    <factor>-25</factor>
    <center>
      <x-m>2.652</x-m>
      <y-m>0.0</y-m>
      <z-m>0.271</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>ElevTrim.Wheel</object-name>
    <property>controls/flight/elevator-trim</property>
    <factor>-1000</factor>
    <center>
      <x-m>2.679713</x-m>
      <y-m>-0.103984</y-m>
      <z-m>0.270674</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/flight/elevator-trim</property>
        <factor>-0.001</factor>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ThrLever01</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor>54</factor>
    <center>
      <x-m>2.652</x-m>
      <y-m>0.0</y-m>
      <z-m>0.271</z-m>
    </center>
