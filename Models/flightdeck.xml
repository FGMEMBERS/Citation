      </binding>
    </hovered>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>ThrRevLever01</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.05</interval-sec>
      <binding>
        <condition>
          <and>
            <property>controls/engines/engine[0]/reverser</property>
            <less-than>
              <property>controls/engines/engine[0]/throttle</property>
              <value>1.0</value>
            </less-than>
          </and>
        </condition>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/throttle</property>
        <step>0.01</step>
      </binding>
      <binding>
        <condition>
          <and>
            <not><property>controls/engines/engine[0]/reverser</property></not>
            <less-than>
              <property>controls/engines/engine[0]/throttle-lever</property>
              <value>0.03</value>
            </less-than>
            <greater-than>
              <property>controls/engines/engine[0]/throttle-lever</property>
              <value>-0.01</value>
            </greater-than>
          </and>
        </condition>
        <command>property-assign</command>
        <property>controls/engines/engine[0]/reverser</property>
        <value>1</value>
      </binding>
      <binding>
        <condition>
          <and>
            <not><property>controls/engines/engine[0]/reverser</property></not>
            <greater-than>
              <property>controls/engines/engine[0]/throttle</property>
              <value>0.02</value>
            </greater-than>
          </and>
        </condition>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/throttle</property>
        <step>-0.01</step>
      </binding>
    </action>
    <hovered>
