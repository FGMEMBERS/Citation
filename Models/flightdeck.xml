  <animation>
    <type>select</type>
    <object-name>Yoke</object-name>
    <object-name>Yoke1</object-name>
    <object-name>Yoke2</object-name>
    <condition>
      <property>sim/yokes-visible</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>nose.lamp</object-name>
    <object-name>LH.lamp</object-name>
    <object-name>RH.lamp</object-name>
    <condition>
      <greater-than>
        <property>/systems/electrical/ac-volts</property>
        <value>90</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>nose.lamp</object-name>
    <condition>
      <equals>
        <property>gear/gear/position-norm</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LH.lamp</object-name>
    <condition>
      <equals>
        <property>gear/gear[1]/position-norm</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>RH.lamp</object-name>
    <condition>
      <equals>
        <property>gear/gear[2]/position-norm</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Gear.unlocked</object-name>
    <condition>
      <and>
        <less-than>
          <property>gear/gear/position-norm</property>
          <value>1</value>
        </less-than>
        <greater-than>
          <property>gear/gear/position-norm</property>
          <value>0</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Rvsr.arm</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/reverser-norm</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Rvsr.unlock</object-name>
