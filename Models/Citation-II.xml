    <inherits-from>Aircraft/Citation/Models/Effects/glass/glass</inherits-from>
    <parameters>
      <texture n="3">
        <type>cubemap</type>
        <images>
          <positive-x>Effects/glass/refl_cube_back.png</positive-x>
          <negative-x>Effects/glass/refl_cube_front.png</negative-x>
          <positive-y>Effects/glass/refl_cube_right.png</positive-y>
          <negative-y>Effects/glass/refl_cube_left.png</negative-y>
          <positive-z>Effects/glass/refl_cube_up.png</positive-z>
          <negative-z>Effects/glass/refl_cube_down.png</negative-z>
        </images>
      </texture>
      <use-reflection type="int">1</use-reflection>
      <reflection-strength type="float">0.5</reflection-strength>
    </parameters>
    <object-name>Door.glass.inside</object-name>
    <object-name>Glass.001.inside</object-name>
    <object-name>Glass.inside</object-name>
  </effect>

