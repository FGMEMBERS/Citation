  <!-- Hide pilot in pilots view and both pilots in Center Console View -->
  <animation>
    <type>select</type>
    <object-name>pilot</object-name>
    <condition>
      <and>
        <property>controls/human-models</property>
        <not-equals>
          <property>sim/current-view/name</property>
	  <value>Captain</value>
        </not-equals>
        <not-equals>
          <property>sim/current-view/name</property>
	  <value>Center Console</value>
        </not-equals>
        <not>
	  <property>sim/walker/outside</property>
