 <!--                        Shadow Map                    -->
<!-- pitch -->
 <animation>
  <type>rotate</type>
  <object-name>Shadow</object-name>
  <property>/orientation/pitch-deg</property>
  <factor>-1.0</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<!-- roll -->
 <animation>
  <type>rotate</type>
  <object-name>Shadow</object-name>
  <property>/orientation/roll-deg</property>
  <factor>1.0</factor>
  <center>
   <x-m>0</x-m>
