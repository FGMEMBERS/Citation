    </offsets>
    <nopreview/>
  </model>
    <!-- Hide copilot in copilots view and both pilots in Center Console View OR hide copilot if crew-weight is less than 200lbs -->
  <animation>
    <type>select</type>
    <object-name>copilot</object-name>
    <condition>
      <and>
	<property>controls/human-models</property>
        <not-equals>
          <property>sim/current-view/name</property>
          <value>Copilot</value>
        </not-equals>
        <not-equals>
          <property>sim/current-view/name</property>
          <value>Center Console</value>
        </not-equals>
        <greater-than>
          <property>sim/multiplay/generic/float[9]</property>
          <value>200lbs</value>
        </greater-than>
      </and>
    </condition>
  </animation>

