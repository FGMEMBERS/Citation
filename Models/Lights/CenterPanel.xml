<?xml version="1.0"?>

<PropertyList>

 <path>conesCenterPanel.ac</path>

     <animation>
        <type>select</type>
        <object-name>IntLight.CenterPanel.ConeLeft</object-name>
        <condition>
            <and>
		<property>sim/rendering/rembrandt/enabled</property>
		<property>controls/lighting/PanelLightsSwitch</property>
		<greater-than>
                    <property>/sim/rendering/shaders/lights</property>
                    <value>2.0</value>
                </greater-than>
                <greater-than>
                    <property>systems/electrical/left-bus</property>
                    <value>5</value>
                </greater-than>
            </and>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>IntLight.CenterPanel.ConeRight</object-name>
        <condition>
            <and>
		<property>sim/rendering/rembrandt/enabled</property>
		<property>controls/lighting/PanelLightsSwitch</property>
		<greater-than>
                    <property>/sim/rendering/shaders/lights</property>
                    <value>2.0</value>
                </greater-than>
                <greater-than>
                    <property>systems/electrical/left-bus</property>
                    <value>5</value>
                </greater-than>
            </and>
        </condition>
    </animation>
    
    <animation>
	<type>light</type>
	<light-type>spot</light-type>
	<name>CenterPanelLightLeft</name>
	<object-name>IntLight.CenterPanel.ConeLeft</object-name>
	<nopreview/>
	<position>
		<x>2.42637</x>
		<y>-0.12653</y>
		<z>0.80347</z>
	</position>
	<direction>
		<x>-1.37280</x>
		<y>0.29</y>
		<z>-1.06451</z>
	</direction>
	<ambient>
		<r>0.9</r>
		<g>0.6</g>
		<b>0.4</b>
		<a>0.5</a>
	</ambient>
	<diffuse>
		<r>0.7</r>
		<g>0.4</g>
		<b>0.3</b>
		<a>0.7</a>
	</diffuse>
	<specular>
		<r>0.7</r>
		<g>0.4</g>
		<b>0.3</b>
		<a>0.9</a>
	</specular>
	<dim-factor>
		<property>controls/lighting/CenterPanelFactor</property>
		<factor>1</factor>
		<offset>0</offset>
		<min>0</min>
		<max>1</max>
	</dim-factor>
	<attenuation>
		<c>0.0</c>
		<l>8</l>
		<q>80</q>
	</attenuation>
	<cutoff>70</cutoff>
	<near-m>0.001</near-m>
	<far-m>2</far-m>
    </animation>
    <animation>
	<type>light</type>
	<light-type>spot</light-type>
	<name>CenterPanelLightRight</name>
	<object-name>IntLight.CenterPanel.ConeRight</object-name>
	<nopreview/>
	<position>
		<x>2.42637</x>
		<y>0.12653</y>
		<z>0.80347</z>
	</position>
	<direction>
		<x>-1.37280</x>
		<y>-0.29</y>
		<z>-1.06451</z>
