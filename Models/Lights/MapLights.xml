<?xml version="1.0"?>

<PropertyList>

 <path>coneMapLights.ac</path>

     <animation>
        <type>select</type>
        <object-name>IntLight.MapLightLeft.cone</object-name>
        <object-name>IntLight.MapLightRight.cone</object-name>
        <condition>
            <and>
		<property>sim/rendering/rembrandt/enabled</property>
		<property>systems/electrical/circuit-breakers/bus-right/cb-light-panel-right</property>
		<property>controls/lighting/map-lights</property>
		<greater-than>
                    <property>sim/rendering/shaders/lights</property>
                    <value>3.0</value>
                </greater-than>
		<greater-than>
                    <property>systems/electrical/right-bus</property>
                    <value>5</value>
                </greater-than>
            </and>
        </condition>
    </animation>

    <animation>
	<type>light</type>
	<light-type>spot</light-type>
	<name>MapLightLeft</name>
	<object-name>IntLight.MapLightLeft.cone</object-name>
	<nopreview/>
	<position>
		<x>2.94338</x>
		<y>-0.30013</y>
		<z>1.17051</z>
	</position>
	<direction>
		<x>-0.08892</x>
		<y>-0.11774</y>
		<z>-1</z>
	</direction>
	<ambient>
		<r>0.9</r>
		<g>0.7</g>
		<b>0.5</b>
		<a>1</a>
	</ambient>
	<diffuse>
		<r>0.7</r>
		<g>0.4</g>
		<b>0.3</b>
		<a>1</a>
	</diffuse>
	<specular>
		<r>0.7</r>
		<g>0.4</g>
		<b>0.3</b>
		<a>1</a>
	</specular>
	<attenuation>
		<c>1.0</c>
		<l>2</l>
		<q>3</q>
	</attenuation>
	<cutoff>35</cutoff>
	<near-m>0.001</near-m>
	<far-m>2</far-m>
    </animation>
