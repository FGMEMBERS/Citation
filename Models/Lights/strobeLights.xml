<?xml version='1.0' encoding='utf-8'?>

<PropertyList>

    <path>sphereStrobeLights.ac</path>

    <animation>
        <type>select</type>
        <object-name>sphereStrobeLight.left</object-name>
        <object-name>sphereStrobeLight.right</object-name>
        <condition>
            <and>
                <property>controls/lighting/strobe</property>
                <property>sim/rendering/rembrandt/enabled</property>
                <greater-than>
                    <property>sim/rendering/shaders/lights</property>
                    <value>2.0</value>
                </greater-than>
                <greater-than>
                    <property>systems/electrical/outputs/strobe</property>
                    <value>20</value>
                </greater-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>light</type>
        <light-type>point</light-type>
        <name>StrobeLightLeft.light</name>
        <object-name>sphereStrobeLight.left</object-name>
        <nopreview/>
        <position>
            <x>6.92</x>
            <y>-7.82</y>
            <z>0.407</z>
        </position>
        <direction>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </direction>
        <ambient>
            <r>1</r>
            <g>1</g>
            <b>1</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.55</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.55</b>
            <a>1</a>
        </specular>
        <attenuation>
            <c>3.0</c>
            <l>2</l>
            <q>4</q>
        </attenuation>
        <near-m>0.1</near-m>
        <far-m>50</far-m>
    </animation>

    <animation>
        <type>light</type>
        <light-type>point</light-type>
        <name>StrobeLightRight.light</name>
        <object-name>sphereStrobeLight.right</object-name>
        <nopreview/>
        <position>
            <x>6.92</x>
            <y>7.82</y>
            <z>0.407</z>
        </position>
        <direction>
            <x>0.0</x>
            <y>1.0</y>
            <z>0.0</z>
        </direction>
        <ambient>
            <r>1</r>
            <g>1</g>
            <b>1</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.55</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.55</b>
            <a>1</a>
        </specular>
        <attenuation>
            <c>3.0</c>
            <l>2</l>
            <q>4</q>
        </attenuation>
        <near-m>0.1</near-m>
        <far-m>50</far-m>
    </animation>

</PropertyList>
