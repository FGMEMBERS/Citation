<PropertyList>
  <!--
      Exterior Lights
  -->
  <model>
    <name>LandingLights</name>
    <path>Aircraft/Citation/Models/Lights/landingLights.xml</path>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>

  <model>
    <condition>
