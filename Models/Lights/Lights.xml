<PropertyList>
  <!--
      Exterior Lights
  -->
  <model>
    <name>LandingLights</name>
    <path>Aircraft/Citation/Models/Lights/landingLights.xml</path>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>

  <model>
    <condition>
      <not><property>sim/rendering/shaders/skydome</property></not>
      <property>systems/electrical/outputs/landing-light[0]</property>
    </condition>
    <name>Landing_Light_Non_Rembrandt_Left</name>
    <path>Aircraft/Citation/Models/Lights/Landing_Lights_Non_Rembrandt.xml</path>
    <offsets>
      <x-m>7.015</x-m>
      <y-m>-2.85</y-m>
      <z-m>-0.34</z-m>
      <pitch-deg>-12</pitch-deg>
      <heading-deg>-3</heading-deg>
    </offsets>
  </model>

