    </condition>
    <name>Landing_Light_Non_Rembrandt_Left</name>
    <path>Aircraft/Citation/Models/Lights/Landing_Lights_Non_Rembrandt.xml</path>
    <offsets>
      <x-m>6.98244</x-m>
      <y-m>-2.81033</y-m>
      <z-m>-0.3416805</z-m>
      <pitch-deg>-12</pitch-deg>
      <heading-deg>-4</heading-deg>
    </offsets>
    <overlay>
      <landing-light>systems/electrical/outputs/landing-light[0]</landing-light>
      <bus-volts>systems/electrical/outputs/bus/left</bus-volts>
    </overlay>
  </model>

