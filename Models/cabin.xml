        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Table.3.in</object-name>
    <property>/controls/table[3]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>Table.3.out</object-name>
    <property>/controls/table[3]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>
  
  <!-- The cup-holders serve as magick buttons to lower (stow) or
       raise (deploy) the tables. All cup-holders act as one object,
       so act on all tables that are in the vertical position. -->
  
  <animation>
    <type>pick</type>
    <object-name>Cabin.Cupholders</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <condition>
          <greater-than>
            <property>/controls/table[1]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>/controls/table[1]/slide</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>/controls/table[2]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>/controls/table[2]/slide</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>/controls/table[3]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>/controls/table[3]/slide</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <binding>
        <condition>
          <greater-than>
            <property>/controls/table[1]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>/controls/table[1]/slide</property>
        <step>-0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>/controls/table[2]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>/controls/table[2]/slide</property>
        <step>-0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>/controls/table[3]/out</property>
            <value>89</value>
          </greater-than>
