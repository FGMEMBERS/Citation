  <animation>
    <type>rotate</type>
    <object-name>Table.3.in</object-name>
    <property>/controls/table[3]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>-0.616012</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>Table.3.out</object-name>
    <property>/controls/table[3]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>-0.616012</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
