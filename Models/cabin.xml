  </animation>

  <!-- The outer half of the table, additionally, rotates the inner
       part which is attached to it -->
  <animation>
    <type>rotate</type>
    <object-name>Table.2.in</object-name>
    <property>/controls/table[2]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>0.61663</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>Table.2.out</object-name>
    <property>/controls/table[2]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>0.61663</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>/controls/table[2]/slide</property>
            <value>0.01</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>/controls/table[2]/out</property>
        <factor>10</factor>
        <min>0</min>
        <max>90</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Table.2.in</object-name>
    <property>/controls/table[2]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>Table.2.out</object-name>
    <property>/controls/table[2]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <!-- Table 3 between rear left seats -->

  <animation>
    <type>knob</type>
    <object-name>Table.3.in</object-name>
    <property>/controls/table[3]/in</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>-0.372027</y-m>
      <z-m>0.503988</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>/controls/table[3]/out</property>
