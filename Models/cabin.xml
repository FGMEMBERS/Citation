<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>

  <path>cabin.ac</path>

  <animation>
    <!-- Do not show the following objects in the 3D preview of fgrun: -->
    <object-name>Ladder</object-name>
    <object-name>Ladder.lRope</object-name>
    <object-name>Ladder.rRope</object-name>
    <nopreview/>
  </animation>

  <effect>  <!-- Chrome Effect for interior parts -->
    <inherits-from>Models/Effects/citationII-chrome-interior</inherits-from>
    <object-name>Ladder.lRope</object-name>
    <object-name>Ladder.rRope</object-name>
    <object-name>RfrshmntCntr.handles</object-name>
    <object-name>Cabin.FloorAirVents</object-name>
    <object-name>Cabin.Cupholders</object-name>
    <object-name>Seat.levers</object-name>
    <object-name>Seats.mountLedges</object-name>
    <object-name>Cabin.MapLights</object-name>
    <object-name>Cabin.MapLights.ins</object-name>
    <object-name>Cabin.MapLights.Bulb</object-name>
  </effect>

  <effect> <!-- glass -->
    <inherits-from>Effects/model-transparent</inherits-from>
  </effect>

  <animation>
    <type>rotate</type>
    <object-name>Cabin.door1</object-name>
    <property>sim/multiplay/generic/float</property>
    <factor>120</factor>
    <axis>
      <x1-m>4.071</x1-m>
      <y1-m>-0.768</y1-m>
      <z1-m>0.617</z1-m>
      <x2-m>4.071</x2-m>
      <y2-m>-0.769</y2-m>
      <z2-m>0.397</z2-m>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Ladder.lRope</object-name>
    <object-name>Ladder.rRope</object-name>
    <condition>
      <greater-than>
        <property>sim/multiplay/generic/float</property>
        <value>0.9</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Ladder</object-name>
    <property>sim/multiplay/generic/float</property>
    <factor>-140</factor>
    <offset-deg>140</offset-deg>
    <center>
      <x-m>4.651144</x-m>
      <y-m>-0.456012</y-m>
      <z-m>-0.089344</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <!-- Table 1 between forward seats -->

  <animation>
    <type>knob</type>
    <object-name>Table.1.in</object-name>
    <property>controls/table[1]/in</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>4.632058</x-m>
      <y-m>0.372647</y-m>
      <z-m>0.503988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>controls/table[1]/out</property>
            <value>10</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[1]/in</property>
        <factor>10</factor>
        <min>0</min>
        <max>180</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <!-- The outer half of the table, additionally, rotates the inner
       part which is attached to it -->
  <animation>
    <type>rotate</type>
    <object-name>Table.1.in</object-name>
    <property>controls/table[1]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>4.632058</x-m>
      <y-m>0.61663</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>Table.1.out</object-name>
    <property>controls/table[1]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>4.632058</x-m>
      <y-m>0.61663</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>controls/table[1]/slide</property>
            <value>0.01</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[1]/out</property>
        <factor>10</factor>
        <min>0</min>
        <max>90</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Table.1.in</object-name>
    <property>controls/table[1]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>Table.1.out</object-name>
    <property>controls/table[1]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <!-- Table 2 between rear right seats -->

  <animation>
    <type>knob</type>
    <object-name>Table.2.in</object-name>
    <property>controls/table[2]/in</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>0.372647</y-m>
      <z-m>0.503988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>controls/table[2]/out</property>
            <value>10</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[2]/in</property>
        <factor>10</factor>
        <min>0</min>
        <max>180</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <!-- The outer half of the table, additionally, rotates the inner
       part which is attached to it -->
  <animation>
    <type>rotate</type>
    <object-name>Table.2.in</object-name>
    <property>controls/table[2]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>0.61663</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>Table.2.out</object-name>
    <property>controls/table[2]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>0.61663</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>controls/table[2]/slide</property>
            <value>0.01</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[2]/out</property>
        <factor>10</factor>
        <min>0</min>
        <max>90</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Table.2.in</object-name>
    <property>controls/table[2]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>Table.2.out</object-name>
    <property>controls/table[2]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <!-- Table 3 between rear left seats -->

  <animation>
    <type>knob</type>
    <object-name>Table.3.in</object-name>
    <property>controls/table[3]/in</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>-0.372027</y-m>
      <z-m>0.503988</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>controls/table[3]/out</property>
            <value>10</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[3]/in</property>
        <factor>10</factor>
        <min>0</min>
        <max>180</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <!-- The outer half of the table, additionally, rotates the inner
       part which is attached to it -->
  <animation>
    <type>rotate</type>
    <object-name>Table.3.in</object-name>
    <property>controls/table[3]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>-0.616012</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>Table.3.out</object-name>
    <property>controls/table[3]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>-0.616012</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>controls/table[3]/slide</property>
            <value>0.01</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[3]/out</property>
        <factor>10</factor>
        <min>0</min>
        <max>90</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Table.3.in</object-name>
    <property>controls/table[3]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>Table.3.out</object-name>
    <property>controls/table[3]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <!-- The cup-holders serve as magick buttons to lower (stow) or
       raise (deploy) the tables. All cup-holders act as one object,
       so act on all tables that are in the vertical position. -->

  <animation>
    <type>pick</type>
    <object-name>Cabin.Cupholders</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[1]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[1]/slide</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[2]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[2]/slide</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[3]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[3]/slide</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[1]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[1]/slide</property>
        <step>-0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[2]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[2]/slide</property>
        <step>-0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[3]/out</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[3]/slide</property>
        <step>-0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>material</type>
    <condition>
      <greater-than>
        <property>controls/lighting/cabin-lights-norm</property>
	<value>0.05</value>
      </greater-than>
      <greater-than>
        <property>systems/electrical/left-bus</property>
        <value>5</value>
      </greater-than>
    </condition>
    <object-name>Cabin</object-name>
    <object-name>Cabin.AftComp.Door</object-name>
    <object-name>Cabin.Cupholders</object-name>
    <object-name>Cabin.DoorFrame</object-name>
    <object-name>Cabin.Floor</object-name>
    <object-name>Cabin.FllorAirVents</object-name>
    <object-name>Cabin.OHPanel</object-name>
    <object-name>Cabin.door1</object-name>
    <object-name>Ladder</object-name>
    <object-name>Ladder.Anchors</object-name>
    <object-name>RfrshmntCntr</object-name>
    <object-name>RfrshmntCntr.handles</object-name>
    <object-name>Seat.1</object-name>
    <object-name>Seat.2</object-name>
    <object-name>Seat.3</object-name>
    <object-name>Seat.4</object-name>
    <object-name>Seat.5</object-name>
    <object-name>Seat.6</object-name>
    <object-name>Seat.7</object-name>
    <object-name>Seat.levers</object-name>
    <object-name>Seat.mountLedges</object-name>
    <object-name>Table.1.in</object-name>
    <object-name>Table.1.out</object-name>
    <object-name>Table.2.in</object-name>
    <object-name>Table.2.out</object-name>
    <object-name>Table.3.in</object-name>
    <object-name>Table.3.out</object-name>
    <object-name>TableCasings</object-name>
    <object-name>Cabin.MapLights</object-name>
    <object-name>Cabin.LightKnob</object-name>
    <object-name>Cabin.LightKnob.Plate</object-name>
    <emission>
      <red>0.3</red>
      <green>0.2</green>
      <blue>0.16</blue>
      <factor-prop>controls/lighting/cabin-lights-norm</factor-prop>
    </emission>
  </animation>

  <!-- Cabin Map Lights knob and animations -->
  <animation>
    <type>knob</type>
    <object-name>Cabin.LightKnob</object-name>
    <property>controls/lighting/cabin-lights-norm</property>
    <factor>135</factor>
    <center>
      <x-m>4.27121</x-m>
      <y-m>-0.57671</y-m>
      <z-m>0.85835</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/lighting/cabin-lights-norm</property>
        <factor>0.1</factor>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>Cabin.LightKnob.Plate</object-name>
    <property>controls/lighting/cabin-lights-norm</property>
    <factor>135</factor>
    <action>
      <button>0</button>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/lighting/cabin-lights-norm</property>
        <step>0.1</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/lighting/cabin-lights-norm</property>
        <step>-0.1</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>material</type>
    <condition>
      <greater-than>
        <property>controls/lighting/cabin-lights-norm</property>
        <value>0.35</value>
      </greater-than>
      <greater-than>
        <property>systems/electrical/right-bus</property>
        <value>5</value>
      </greater-than>
    </condition>
    <object-name>Cabin.MapLights.Bulb</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>controls/lighting/cabin-lights-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <condition>
      <greater-than>
        <property>controls/lighting/cabin-lights-norm</property>
	<value>0.35</value>
      </greater-than>
      <greater-than>
        <property>systems/electrical/right-bus</property>
        <value>5</value>
      </greater-than>
    </condition>
    <object-name>Cabin.MapLights.ins</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>controls/lighting/cabin-lights-norm</factor-prop>
    </emission>
  </animation>

</PropertyList>
