<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>

  <path>cabin.ac</path>

  <effect>  <!-- Chrome Effect! -->
    <inherits-from>Models/Effects/citationII-chrome</inherits-from>
    <object-name>Ladder.lRope</object-name>
    <object-name>Ladder.rRope</object-name>
    <object-name>RfrshmntCntr.handles</object-name>
    <object-name>Cabin.FloorAirVents</object-name>
    <object-name>Cabin.Cupholders</object-name>
    <object-name>Seat.levers</object-name>
  </effect>
  
  <animation>
    <type>rotate</type>
    <object-name>Cabin.door1</object-name>
    <property>sim/multiplay/generic/float</property>
    <factor>120</factor>
    <axis>
      <x1-m>4.071</x1-m>
      <y1-m>-0.768</y1-m>
      <z1-m>0.617</z1-m>
      <x2-m>4.071</x2-m>
      <y2-m>-0.769</y2-m>
      <z2-m>0.397</z2-m>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Ladder.lRope</object-name>
    <object-name>Ladder.rRope</object-name>
    <condition>
      <greater-than>
        <property>sim/multiplay/generic/float</property>
        <value>0.9</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Ladder</object-name>
    <property>sim/multiplay/generic/float</property>
    <factor>-140</factor>
    <offset-deg>140</offset-deg>
    <center>
      <x-m>4.651144</x-m>
      <y-m>-0.456012</y-m>
      <z-m>-0.089344</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <!-- Table 1 between forward seats -->

  <animation>
    <type>knob</type>
    <object-name>Table.1.in</object-name>
    <property>/controls/table[1]/in</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>4.632058</x-m>
      <y-m>0.372647</y-m>
      <z-m>0.503988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>/controls/table[1]/out</property>
            <value>10</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>/controls/table[1]/in</property>
        <factor>10</factor>
        <min>0</min>
        <max>180</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <!-- The outer half of the table, additionally, rotates the inner
       part which is attached to it -->
  <animation>
    <type>rotate</type>
    <object-name>Table.1.in</object-name>
    <property>/controls/table[1]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>4.632058</x-m>
      <y-m>0.61663</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>Table.1.out</object-name>
    <property>/controls/table[1]/out</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>4.632058</x-m>
      <y-m>0.61663</y-m>
      <z-m>0.498988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>/controls/table[1]/slide</property>
            <value>0.01</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>/controls/table[1]/out</property>
        <factor>10</factor>
        <min>0</min>
        <max>90</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Table.1.in</object-name>
    <property>/controls/table[1]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>Table.1.out</object-name>
    <property>/controls/table[1]/slide</property>
    <factor>1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <!-- Table 2 between rear right seats -->

  <animation>
    <type>knob</type>
    <object-name>Table.2.in</object-name>
    <property>/controls/table[2]/in</property>
    <factor>1</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>6.496685</x-m>
      <y-m>0.372647</y-m>
      <z-m>0.503988</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <condition>
          <less-than>
            <property>/controls/table[2]/out</property>
