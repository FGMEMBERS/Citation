    <!--
    -1 LH GEN
     0 BATT
     1 RH GEN
    -->
    <animation>
        <type>knob</type>
        <object-name>volt.knob</object-name>
        <property>controls/electric/voltage-sel</property>
        <factor>30</factor>
        <center>
            <x-m>0.01023</x-m>
            <y-m>-0.04251</y-m>
            <z-m>0.07432</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/electric/voltage-sel</property>
                <factor>1</factor>
                <min>-1</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
              <command>set-tooltip</command>
              <tooltip-id>voltage-sel</tooltip-id>
              <label>Voltage selector: %s</label>
              <mapping>nasal</mapping>
              <script>
                var modes = ['Battery', 'Right hand generator', 'Left hand generator'];
                return modes[arg[0]];
              </script>
              <property>controls/electric/voltage-sel</property>
            </binding>
        </hovered>
    </animation>

    <animation>
      <type>rotate</type>
      <object-name>volts.needle</object-name>
      <property>controls/electric/voltage-display</property>
      <factor>8.15</factor>
      <offset-deg>-55</offset-deg>
      <center>
