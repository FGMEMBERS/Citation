    <!--
    1 OFF
    2 ANNU
    3 SPARE
    4 OVERSPEED
    5 W/S TEMP
    6 THRU REV
    7 SPARE
    8 BATT TEMP
    9 LDG GEAR
   10 OFF
    -->

    <animation>
        <type>knob</type>
        <object-name>test.knob</object-name>
        <property>controls/electric/system-test</property>
        <factor>36</factor>
        <center>
            <x-m>0.01023</x-m>
            <y-m>-0.06491</y-m>
            <z-m>0.03725</z-m>
        </center>
