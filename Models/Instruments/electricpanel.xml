<!-- -1 LH GEN / 0 BATT / 1 RH GEN -->
  <animation>
    <type>knob</type>
    <object-name>elec.volt.knob</object-name>
    <property>controls/electric/voltage-sel</property>
    <factor>30</factor>
    <center>
      <x-m>0.0045</x-m>
      <y-m>-0.04250</y-m>
      <z-m>0.07390</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/electric/voltage-sel</property>
        <factor>1</factor>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>voltage-sel</tooltip-id>
        <label>Voltage selector: %s</label>
        <mapping>nasal</mapping>
        <script>
          var modes = ['Battery', 'Right hand generator', 'Left hand generator'];
          return modes[arg[0]];
        </script>
        <property>controls/electric/voltage-sel</property>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>elec.volt.needle</object-name>
    <property>controls/electric/voltage-display</property>
    <factor>8.0</factor>
    <offset-deg>-80</offset-deg>
    <center>
      <x-m>0.00200</x-m>
      <y-m>-0.00160</y-m>
      <z-m>0.07690</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

<!--
1 OFF / 2 ANNU / 3 SPARE / 4 OVERSPEED / 5 W/S TEMP
6 THRU REV / 7 SPARE / 8 BATT TEMP / 9 LDG GEAR / 10 OFF
-->

  <animation>
