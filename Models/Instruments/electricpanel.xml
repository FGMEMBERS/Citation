                <factor>1</factor>
                <min>-2.5</min>
                <max>2.5</max>
                <wrap>true</wrap>
            </binding>
        </action>
        <hovered>
          <binding>
            <command>set-tooltip</command>
            <tooltip-id>fuel-sel</tooltip-id>
            <label>Fuel flow selector: %s (non-functional)</label>
            <mapping>nasal</mapping>
            <script>
              var modes = ['OFF', 'Right hand tank', 'Right hand engine', 'Left hand engine', 'Left hand tank'];
              return modes[arg[0]];
            </script>
            <property>controls/electric/fuel-sel</property>
          </binding>
        </hovered>
    </animation>

