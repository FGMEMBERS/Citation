        <script>
          var modes = ['OFF', 'OFF', 'Landing gear', 'Battery temp', 'Spare', 'Thrust reversers', 'W/S temp', 'Overspeed', 'Spare', 'Annu'];
          return modes[arg[0]];
        </script>
        <property>controls/electric/system-test</property>
