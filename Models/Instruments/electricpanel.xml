            </binding>
            <binding>
                <condition>
                    <property>controls/electric/engine[1]/starter-btn</property>
                </condition>
                <command>property-toggle</command>
                <property>controls/electric/engine[1]/starter-btn</property>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>starter-disengage</tooltip-id>
                <label>Starter disengage</label>
            </binding>
        </hovered>
    </animation>

