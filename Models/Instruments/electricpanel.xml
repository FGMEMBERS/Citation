            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/electric/system-test</property>
                <factor>1</factor>
                <min>1</min>
                <max>10</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
              <command>set-tooltip</command>
              <tooltip-id>electric-systems-test</tooltip-id>
              <label>Electric systems test: %s (non-functional)</label>
              <mapping>nasal</mapping>
              <script>
                var modes = ['', 'OFF', 'Landing gear', 'Battery temp', 'Spare', 'Thrust reversers', 'W/S temp', 'Overspeed', 'Spare', 'Annu', 'OFF'];
                return modes[arg[0]];
              </script>
              <property>controls/electric/system-test</property>
            </binding>
        </hovered>
    </animation>


    <!--
    -2 LH Eng
    -1 LH tank
     0 OFF
     1 RH tank
     2 RH Eng
