<!-- 1 BATT
     0 OFF
    -1 EMER -->
    <animation>
        <type>knob</type>
        <object-name>batt.sw</object-name>
        <property>controls/electric/battery-bus-switch</property>
        <factor>15</factor>
        <center>
            <x-m>0.005</x-m>
            <y-m>0.002</y-m>
            <z-m>0.037</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/electric/battery-bus-switch</property>
                <factor>1</factor>
                <min>-1</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>battery-bus-switch</tooltip-id>
                <label>Battery bus: %s</label>
                <mapping>nasal</mapping>
                <script>
                    var modes = ['OFF', 'BATT', 'EMER'];
                    return modes[arg[0]];
                </script>
                <property>controls/electric/battery-bus-switch</property>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
