<PropertyList>

<path>aoa.ac</path>

  <effect>
    <condition>
      <greater-than>
	<property>sim/version/flightgear</property>
	<value>3.4.999</value>
      </greater-than>
    </condition>
    <inherits-from>Aircraft/Citation/Models/Effects/interior-shadow/citationII-interior-flightdeck</inherits-from>
    <object-name>AOA</object-name>
    <object-name>AOA.face</object-name>
    <object-name>AOA.needle</object-name>
  </effect>

    <animation>
        <type>rotate</type>
        <object-name>AOA.needle</object-name>
        <property>orientation/alpha-deg</property>
			<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>14</ind><dep>226.5</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>-0</z>
        </axis>
    </animation>

</PropertyList>