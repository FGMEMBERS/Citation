<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>

<path>lightingpanel.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>Beacon.sw</object-name>
        <property>controls/lighting/beacon</property>
        <factor>30</factor>
        <center>
            <x-m>0.005</x-m>
            <y-m>0.002</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>LandingLightLeft.sw</object-name>
        <property>controls/lighting/landing-light</property>
        <factor>30</factor>
        <center>
            <x-m>0.005</x-m>
            <y-m>0.002</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>LandingLightRight.sw</object-name>
        <property>controls/lighting/landing-light[1]</property>
        <factor>30</factor>
        <center>
            <x-m>0.005</x-m>
            <y-m>0.002</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>PanelLight.sw</object-name>
        <property>controls/lighting/PanelLightsSwitch</property>
        <factor>30</factor>
        <center>
            <x-m>0.005</x-m>
            <y-m>0.002</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>FloodLight.knob</object-name>
        <property>controls/lighting/FloodLightFactor</property>
        <factor>300</factor>
	<center>
	    <x-m>0</x-m>
            <y-m>-0.11689</y-m>
            <z-m>0.004096</z-m>
	</center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/FloodLightFactor</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>CenterPanelLight.knob</object-name>
        <property>controls/lighting/CenterPanelFactor</property>
        <factor>300</factor>
	<center>
	    <x-m>0</x-m>
            <y-m>-0.0435</y-m>
            <z-m>0.004096</z-m>
	</center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/CenterPanelFactor</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>LeftPanelLight.knob</object-name>
        <property>controls/lighting/LeftPanelFactor</property>
        <factor>300</factor>
	<center>
	    <x-m>0</x-m>
            <y-m>-0.0687</y-m>
            <z-m>0.004096</z-m>
	</center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/LeftPanelFactor</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>RightPanelLight.knob</object-name>
        <property>controls/lighting/RightPanelFactor</property>
        <factor>300</factor>
	<center>
	    <x-m>0</x-m>
            <y-m>-0.01622</y-m>
            <z-m>0.004096</z-m>
	</center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/RightPanelFactor</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

<!-- hotspots -->

<animation>
    <type>pick</type>
    <object-name>Beacon.sw</object-name>
    <action>
        <button>0</button>
        <repeatable>false</repeatable>
        <binding>
            <command>property-toggle</command>
            <property>controls/lighting/beacon</property>
        </binding>
        <binding>
            <command>property-toggle</command>
            <property>sim/sound/click</property>
        </binding>
    </action>
</animation>

