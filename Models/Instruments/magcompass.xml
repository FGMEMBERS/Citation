<?xml version="1.0"?>
<!--
Author : Syd Adams
Citation magnetic compass
-->
<PropertyList>

<path>magcompass.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>compassrose</object-name>
        <property>instrumentation/magnetic-compass/indicated-heading-deg</property>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>1.0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>