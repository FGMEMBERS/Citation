<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>

  <path>ctnpanel.ac</path>

  <effect>
    <condition>
      <greater-than>
	<property>sim/version/flightgear</property>
	<value>3.4.999</value>
      </greater-than>
    </condition>
    <inherits-from>Aircraft/Citation/Models/Effects/interior-shadow/citationII-interior-flightdeck</inherits-from>
    <object-name>ACfail.on</object-name>
    <object-name>Ctn.panel</object-name>
    <object-name>Lfuel.lo</object-name>
    <object-name>Lgen.off</object-name>
    <object-name>Rfuel.lo</object-name>
    <object-name>Rgen.off</object-name>
    <object-name>door.ajar</object-name>
    <object-name>lFpsi.on</object-name>
    <object-name>lHpsi.on</object-name>
    <object-name>lOpsi.on</object-name>
    <object-name>lboost.on</object-name>
    <object-name>rFpsi.on</object-name>
    <object-name>rHpsi.on</object-name>
    <object-name>rOpsi.on</object-name>
    <object-name>rboost.on</object-name>
    <object-name>spdbrake.on</object-name>
  </effect>

  <animation>
    <type>select</type>
    <object-name>spdbrake.on</object-name>
    <condition>
      <property>controls/flight/speedbrake</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Lgen.off</object-name>
    <condition>
      <not>
        <property>controls/electric/engine[0]/generator</property>
      </not>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Rgen.off</object-name>
    <condition>
      <not>
        <property>controls/electric/engine[1]/generator</property>
      </not>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>ACfail.on</object-name>
    <condition>
      <less-than>
        <property>systems/electrical/ac-volts</property>
        <value>15</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>door.ajar</object-name>
    <condition>
      <greater-than>
        <property>controls/cabin-door/position-norm</property>
        <value>0</value>
      </greater-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>lboost.on</object-name>
    <condition>
      <property>controls/fuel/tank[0]/boost-pump</property>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>rboost.on</object-name>
    <condition>
      <property>controls/fuel/tank[1]/boost-pump</property>
      <greater-than>
        <property>systems/electrical/outputs/bus/right</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>lHpsi.on</object-name>
    <condition>
      <less-than>
        <property>engines/engine/turbine</property>
        <value>10</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>rHpsi.on</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/turbine</property>
        <value>10</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>lOpsi.on</object-name>
    <condition>
      <less-than>
        <property>engines/engine/turbine</property>
        <value>20</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>rOpsi.on</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/turbine</property>
        <value>20</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>lFpsi.on</object-name>
    <condition>
      <less-than>
        <property>engines/engine/turbine</property>
        <value>10</value>
      </less-than>
      <not>
	<property>controls/fuel/tank[0]/boost-pump</property>
      </not>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>rFpsi.on</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/turbine</property>
        <value>10</value>
      </less-than>
      <not>
	<property>controls/fuel/tank[1]/boost-pump</property>
      </not>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Lfuel.lo</object-name>
    <condition>
      <less-than>
        <property>consumables/fuel/tank/level-lbs</property>
        <value>190</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Rfuel.lo</object-name>
    <condition>
      <less-than>
        <property>consumables/fuel/tank[1]/level-lbs</property>
        <value>190</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

</PropertyList>
