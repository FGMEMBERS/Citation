  </animation>

  <!-- TODO: should react on oil system -->
  <animation>
    <type>select</type>
    <object-name>OilPressLoR</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/turbine</property>
        <value>20</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelPressLoL</object-name>
    <condition>
      <less-than>
        <property>engines/engine/turbine</property>
        <value>10</value>
      </less-than>
      <not>
        <property>controls/fuel/tank[0]/boost-pump</property>
      </not>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelPressLoR</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/turbine</property>
        <value>10</value>
      </less-than>
      <not>
        <property>controls/fuel/tank[1]/boost-pump</property>
      </not>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelLevelLoL</object-name>
    <condition>
      <less-than>
        <property>consumables/fuel/tank/level-lbs</property>
        <value>190</value>
      </less-than>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelLevelLoR</object-name>
    <condition>
      <less-than>
        <property>consumables/fuel/tank[1]/level-lbs</property>
