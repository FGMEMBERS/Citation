      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>

  <!-- TODO: build up meaningfull contitions to activate this annuntiators -->

  <animation>
    <type>select</type>
    <object-name>ACMEjectorOn</object-name>
    <condition>
      <greater-than>
        <property>systems/electrical/outputs/bus/left</property>
