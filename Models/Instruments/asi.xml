<PropertyList>

  <path>asi.ac</path>

  <effect>
    <condition>
      <greater-than>
	<property>sim/version/flightgear</property>
	<value>3.4.999</value>
      </greater-than>
    </condition>
    <inherits-from>Aircraft/Citation/Models/Effects/interior-shadow/citationII-interior-flightdeck</inherits-from>
    <object-name>ASI320</object-name>
    <object-name>ASI320.face</object-name>
    <object-name>ASI320.knob</object-name>
    <object-name>ASI320.needle</object-name>
    <object-name>ASI320.ovrspeed</object-name>
    <object-name>index.marker</object-name>
    <object-name>mach.subdial</object-name>
  </effect>

  <animation>
    <type>rotate</type>
    <object-name>ASI320.needle</object-name>
    <object-name>mach.subdial</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>0.0</dep></entry>
      <entry><ind>30.0</ind><dep>0.0</dep></entry>
      <entry><ind>40.0</ind><dep>15.0</dep></entry>
      <entry><ind>100.0</ind><dep>82.5</dep></entry>
      <entry><ind>160.0</ind><dep>150.0</dep></entry>
      <entry><ind>240.0</ind><dep>240.0</dep></entry>
      <entry><ind>320.0</ind><dep>330.0</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>mach.subdial</object-name>
    <property>velocities/mach</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>0.0</dep></entry>
      <entry><ind>0.4</ind><dep>264</dep></entry>
      <entry><ind>0.75</ind><dep>474</dep></entry>
    </interpolation>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>index.marker</object-name>
    <property>instrumentation/airspeed-indicator/index-marker</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>0.0</dep></entry>
      <entry><ind>40.0</ind><dep>15.0</dep></entry>
      <entry><ind>100.0</ind><dep>82.5</dep></entry>
      <entry><ind>160.0</ind><dep>150.0</dep></entry>
      <entry><ind>240.0</ind><dep>240.0</dep></entry>
      <entry><ind>320.0</ind><dep>330.0</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>ASI320.knob</object-name>
    <property>instrumentation/airspeed-indicator/index-marker</property>
    <factor>1</factor>
    <center>
      <x-m>0</x-m>
      <y-m>-0.0286915</y-m>
      <z-m>-0.0286915</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/airspeed-indicator/index-marker</property>
        <factor>1</factor>
        <min>0</min>
        <max>320</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

</PropertyList>
