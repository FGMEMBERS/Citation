<?xml version='1.0' encoding='utf-8'?>
<PropertyList>

<!-- attitude -->

    <filter>
        <name>spin</name>
        <debug>false</debug>
        <type>gain</type>
        <input>
            <condition>
                <greater-than>
                    <property>systems/electrical/outputs/AC-26V/ac-adi1</property>
                    <value>18</value>
                </greater-than>
                <greater-than>
                    <property>instrumentation/attitude-indicator[0]/spin</property>
                    <value>0.7</value>
                </greater-than>
            </condition>
            <property>instrumentation/attitude-indicator[0]/spin</property>
        </input>
        <input>0.0</input>
        <output>instrumentation/AD600[0]/spin</output>
    </filter>

    <filter>
        <name>spin</name>
        <debug>false</debug>
        <type>gain</type>
        <input>
            <condition>
                <greater-than>
                    <property>systems/electrical/outputs/AC-26V/ac-adi2</property>
                    <value>18</value>
                </greater-than>
                <greater-than>
                    <property>instrumentation/attitude-indicator[1]/spin</property>
                    <value>0.7</value>
                </greater-than>
            </condition>
            <property>instrumentation/attitude-indicator[1]/spin</property>
        </input>
        <input>0.0</input>
        <output>instrumentation/AD600[1]/spin</output>
    </filter>

<!-- Flight Director -->

    <filter>
        <name>fd-on</name>
        <debug>false</debug>
        <type>gain</type>
        <input>
            <condition>
                <or>
                    <less-than>
                        <property>systems/electrical/outputs/AC-26V/ac-adi1</property>
                        <value>18</value>
                    </less-than>
                    <property>autopilot/locks/passive-mode</property>
                </or>
            </condition>
            <value>0</value>
        </input>
        <input>1</input>
        <output>instrumentation/AD600[0]/fd-on</output>
    </filter>

    <filter>
        <name>fd-roll</name>
        <debug>false</debug>
        <type>noise-spike</type>
        <max-rate-of-change>45</max-rate-of-change>
        <input>
            <condition>
                <greater-than>
                    <property>systems/electrical/outputs/AC-26V/ac-adi1</property>
                    <value>18</value>
                </greater-than>
                <greater-than>
                    <property>instrumentation/attitude-indicator[0]/spin</property>
                    <value>.7</value>
                </greater-than>
                <not><property>autopilot/locks/passive-mode</property></not>
            </condition>
            <expression>
                <dif>
                    <property>autopilot/internal/target-roll-deg</property>
                    <property>instrumentation/attitude-indicator[0]/indicated-roll-deg</property>
                </dif>
            </expression>
        </input>
        <input>0.0</input>
        <min>-45</min>
        <max>45</max>
        <output>instrumentation/AD600[0]/fd-roll-deg</output>
    </filter>

    <filter>
        <name>fd-pitch</name>
        <debug>false</debug>
        <type>noise-spike</type>
        <max-rate-of-change>45</max-rate-of-change>
        <input>
            <condition>
                <greater-than>
                    <property>systems/electrical/outputs/AC-26V/ac-adi1</property>
                    <value>18</value>
                </greater-than>
                <greater-than>
                    <property>instrumentation/attitude-indicator[0]/spin</property>
                    <value>.7</value>
                </greater-than>
                <not><property>autopilot/locks/passive-mode</property></not>
            </condition>
            <expression>
                <sum>
                    <dif>
                        <property>autopilot/internal/target-pitch-deg</property>
                        <property>instrumentation/attitude-indicator[0]/indicated-pitch-deg</property>
                    </dif>
                    <property>orientation/alpha-deg</property>
                </sum>
            </expression>
        </input>
        <input>0.0</input>
        <min>-45</min>
        <max>45</max>
        <output>instrumentation/AD600[0]/fd-pitch-deg</output>
    </filter>

