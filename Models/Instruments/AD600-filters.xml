                <property>instrumentation/nav[1]/in-range</property>
                <property>instrumentation/nav[1]/nav-loc</property>
            </condition>
            <value>1</value>
        </input>
        <input>0</input>
        <output>instrumentation/AD600[0]/valid-loc</output>
    </filter>

