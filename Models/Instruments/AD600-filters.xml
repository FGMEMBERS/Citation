                <property>instrumentation/nav[0]/in-range</property>
                <property>instrumentation/nav[0]/nav-loc</property>
            </condition>
            <value>1</value>
        </input>
        <input>
            <condition>
                <greater-than>
                    <property>systems/electrical/outputs/AC-26V/ac-adi1</property>
                    <value>18</value>
