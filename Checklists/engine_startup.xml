<PropertyList>
  <title>I - Engine startup</title>

  <item>
    <name>Fuel</name>
    <value>As required</value>
    <condition>
      <equals>
        <property>controls/fuelSet</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>dialog-show</command>
      <dialog-name>Citation-II-fuelAndWeight</dialog-name>
    </binding>
  </item>

  <item>
    <name>Battery</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/electric/battery-bus-switch</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/battery-bus-switch</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Left engine starter</name>
    <value>START</value>
    <condition>
      <equals>
        <property>controls/electric/engine[0]/starter-btn</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/engine[0]/starter-btn</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Left cutoff</name>
    <value>UNLOCK</value>
    <condition>
      <equals>
        <property>controls/engines/engine[0]/cutoff-lock</property>
        <value>0</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[0]/cutoff-lock</property>
      <value>0</value>
    </binding>
  </item>

  <item>
    <name>Left engine: n2</name>
    <value>&gt; 8%</value>
    <condition>
      <greater-than-equals>
        <property>engines/engine[0]/turbine</property>
        <value>8</value>
      </greater-than-equals>
    </condition>
  </item>

  <item>
    <name>Left throttle</name>
    <value>IDLE</value>
    <condition>
      <equals>
        <property>controls/engines/engine[0]/throttle</property>
        <value>0.25</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[0]/throttle</property>
      <value>0.25</value>
    </binding>
  </item>

  <item>
    <name>Left engine</name>
    <value>STARTED</value>
    <condition>
      <greater-than-equals>
        <property>engines/engine[0]/started</property>
        <value>1</value>
      </greater-than-equals>
    </condition>
  </item>

  <item>
    <name>Left engine generator</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/electric/engine[0]/generator</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/engine[0]/generator</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Right engine starter</name>
    <value>START</value>
    <condition>
      <equals>
        <property>controls/electric/engine[1]/starter-btn</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/engine[1]/starter-btn</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Right cutoff</name>
    <value>UNLOCK</value>
    <condition>
      <equals>
        <property>controls/engines/engine[1]/cutoff-lock</property>
        <value>0</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[1]/cutoff-lock</property>
      <value>0</value>
    </binding>
  </item>

  <item>
    <name>Right engine: n2</name>
    <value>&gt; 8%</value>
    <condition>
      <greater-than-equals>
        <property>engines/engine[1]/turbine</property>
        <value>8</value>
      </greater-than-equals>
    </condition>
  </item>

  <item>
    <name>Right throttle</name>
    <value>IDLE</value>
    <condition>
      <equals>
        <property>controls/engines/engine[1]/throttle</property>
        <value>0.25</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[1]/throttle</property>
      <value>0.25</value>
    </binding>
  </item>

  <item>
    <name>Right engine</name>
    <value>STARTED</value>
    <condition>
      <greater-than-equals>
        <property>engines/engine[1]/started</property>
        <value>1</value>
      </greater-than-equals>
    </condition>
  </item>

  <item>
    <name>Right engine generator</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/electric/engine[1]/generator</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/engine[1]/generator</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Avionics switch</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/electric/avionics-switch</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/avionics-switch</property>
      <value>1</value>
    </binding>
  </item>

</PropertyList>
