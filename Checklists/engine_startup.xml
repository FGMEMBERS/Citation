<PropertyList>
  <title>I - Engine startup</title>

  <item>
    <name>Fuel</name>
    <value>As required</value>
    <condition>
      <equals>
        <property>controls/fuelSet</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>dialog-show</command>
      <dialog-name>Citation-II-fuelAndWeight</dialog-name>
    </binding>
  </item>

  <item>
    <name>Battery</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/electric/battery-bus-switch</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/battery-bus-switch</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Left ignition</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/engines/engine[0]/ignition</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[0]/ignition</property>
      <value>1</value>
    </binding>
  </item>
  <item>
    <name>Left fuel pump</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/fuel/tank[0]/boost-pump</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/fuel/tank[0]/boost-pump</property>
      <value>1</value>
    </binding>
  </item>
  <item>
    <name>Left engine starter</name>
    <value>START</value>
    <condition>
      <equals>
        <property>controls/engines/engine[0]/starter</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[0]/starter</property>
      <value>1</value>
    </binding>
  </item>
  <item>
    <name>Left engine: n2</name>
    <value>&gt; 20%</value>
    <condition>
      <greater-than-equals>
        <property>engines/engine[0]/turbine</property>
        <value>20</value>
      </greater-than-equals>
    </condition>
  </item>
  <item>
    <name>Left engine generator</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/electric/engine[0]/generator</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/engine[0]/generator</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Right ignition</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/engines/engine[1]/ignition</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[1]/ignition</property>
      <value>1</value>
    </binding>
  </item>
  <item>
    <name>Right fuel pump</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/fuel/tank[1]/boost-pump</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/fuel/tank[1]/boost-pump</property>
      <value>1</value>
    </binding>
  </item>
  <item>
    <name>Right engine starter</name>
    <value>START</value>
    <condition>
      <equals>
        <property>controls/engines/engine[1]/starter</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[1]/starter</property>
      <value>1</value>
    </binding>
  </item>
  <item>
    <name>Right engine: n2</name>
    <value>&gt; 20%</value>
    <condition>
      <greater-than-equals>
        <property>engines/engine[1]/turbine</property>
        <value>20</value>
      </greater-than-equals>
    </condition>
  </item>
  <item>
    <name>Right engine generator</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/electric/engine[1]/generator</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/engine[1]/generator</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Avionics switch</name>
    <value>ON</value>
    <condition>
      <equals>
        <property>controls/electric/avionics-switch</property>
        <value>1</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/electric/avionics-switch</property>
      <value>1</value>
    </binding>
  </item>

  <item>
    <name>Left engine: n1</name>
    <value>&gt; 44%</value>
    <condition>
      <greater-than-equals>
        <property>engines/engine[0]/fan</property>
        <value>44</value>
      </greater-than-equals>
    </condition>
  </item>
  <item>
    <name>Left fuel pump</name>
    <value>OFF</value>
    <condition>
      <equals>
        <property>controls/fuel/tank[0]/boost-pump</property>
        <value>0</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/fuel/tank[0]/boost-pump</property>
      <value>0</value>
    </binding>
  </item>

  <item>
    <name>Right engine: n1</name>
    <value>&gt; 44%</value>
    <condition>
      <greater-than-equals>
        <property>engines/engine[1]/fan</property>
        <value>44</value>
      </greater-than-equals>
    </condition>
  </item>
  <item>
    <name>Right fuel pump</name>
    <value>OFF</value>
    <condition>
      <equals>
        <property>controls/fuel/tank[1]/boost-pump</property>
        <value>0</value>
      </equals>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/fuel/tank[1]/boost-pump</property>
      <value>0</value>
    </binding>
  </item>
</PropertyList>
