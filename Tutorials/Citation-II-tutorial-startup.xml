      <y-m>-0.0658</y-m>
      <z-m>0.2998</z-m>
      <scale>1.0000</scale>
    </marker>

    <exit>
      <condition>
        <equals>
          <property>controls/flight/speedbrake-switch</property>
          <value>0</value>
        </equals>
      </condition>
    </exit>
  </step>

  <step>
    <message>Switch on the beacon light!</message>

