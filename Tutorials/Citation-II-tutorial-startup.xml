      <y-m>-0.6208</y-m>
      <z-m>0.4335</z-m>
      <scale>1.4959</scale>
    </marker>

    <exit>
      <condition>
        <equals>
          <property>controls/fuel/tank[1]/boost-pump</property>
          <value>0</value>
        </equals>
      </condition>
    </exit>
