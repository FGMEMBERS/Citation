    <marker>
      <x-m>2.4679</x-m>
      <y-m>-0.5862</y-m>
      <z-m>0.4742</z-m>
      <scale>1.4959</scale>
    </marker>

    <exit>
      <condition>
        <equals>
          <property>controls/electric/engine[0]/generator</property>
          <value>1</value>
        </equals>
      </condition>
    </exit>
  </step>

  <step>
    <message>If the voltmeter shows about 28V, you can go on with the checklist.</message>

    <view>
      <heading-offset-deg>17.2</heading-offset-deg>
      <pitch-offset-deg>-35.2</pitch-offset-deg>
      <roll-offset-deg>0.0</roll-offset-deg>
      <x-offset-m>-0.3</x-offset-m>
      <y-offset-m>1.1</y-offset-m>
      <z-offset-m>3.3</z-offset-m>
      <field-of-view>22.2</field-of-view>
    </view>

    <marker>
      <x-m>2.4570</x-m>
      <y-m>-0.5612</y-m>
      <z-m>0.5132</z-m>
      <scale>2.1405</scale>
    </marker>

    <exit>
      <condition>
        <greater-than>
          <property>controls/electric/voltage-display</property>
          <value>25</value>
        </greater-than>
      </condition>
    </exit>
  </step>

  <step>
    <message>Now the right engine! Activate right ignition!</message>

    <view>
      <heading-offset-deg>17.2</heading-offset-deg>
      <pitch-offset-deg>-35.2</pitch-offset-deg>
      <roll-offset-deg>0.0</roll-offset-deg>
      <x-offset-m>-0.3</x-offset-m>
      <y-offset-m>1.1</y-offset-m>
      <z-offset-m>3.3</z-offset-m>
