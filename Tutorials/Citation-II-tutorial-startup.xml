      <y-m>-0.2377</y-m>
      <z-m>0.6402</z-m>
      <scale>1.0000</scale>
    </marker>

    <exit>
      <condition>
        <not>
          <equals>
            <property>instrumentation/altimeter/setting-inhg</property>
            <value>29.92</value>
          </equals>
        </not>
      </condition>
    </exit>
  </step>

  <step>
    <message>Check speedbrakes retracted</message>

    <view>
      <heading-offset-deg>338.2</heading-offset-deg>
