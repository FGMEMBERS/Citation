    <marker>
      <x-m>2.4731</x-m>
      <y-m>-0.4802</y-m>
      <z-m>0.4344</z-m>
      <scale>1.4959</scale>
    </marker>

    <exit>
      <condition>
        <equals>
          <property>controls/engines/engine[1]/ignition</property>
          <value>1</value>
        </equals>
      </condition>
