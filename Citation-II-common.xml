      <key n="12">
        <name>Ctrl-L</name>
        <desc>Toggle landing lights</desc>
        <binding>
          <command>property-toggle</command>
          <property>controls/lighting/landing-light-switch</property>
        </binding>
        <binding>
          <command>property-toggle</command>
          <property>controls/lighting/landing-light-switch[1]</property>
        </binding>
      </key>
      <key n="121">
        <name>y</name>
        <desc>Show/Hide Yokes</desc>
        <binding>
          <command>property-toggle</command>
          <property>sim/yokes-visible</property>
        </binding>
      </key>
      <key n="127">
        <name>Delete</name>
        <desc>Toggle Reversers</desc>
        <binding>
	  <condition>
	    <and>
	      <property>controls/electric/circuit-breakers/bus-left/cb-thrust-reverser-left</property>
	      <greater-than>
		<property>systems/electrical/outputs/bus/left</property>
		<value>20</value>
	      </greater-than>
	    </and>
	  </condition>
          <command>property-toggle</command>
          <property>controls/engines/engine[0]/reverser</property>
        </binding>
        <binding>
	  <condition>
	    <and>
	      <property>controls/electric/circuit-breakers/bus-right/cb-thrust-reverser-right</property>
	      <greater-than>
		<property>systems/electrical/outputs/bus/right</property>
		<value>20</value>
	      </greater-than>
	    </and>
	  </condition>
          <command>property-toggle</command>
          <property>controls/engines/engine[1]/reverser</property>
        </binding>
      </key>
      <key n="4">
        <name>Ctrl-d</name>
        <desc>Open/close cabin-door</desc>
        <binding>
          <condition>
            <less-than>
              <property>velocities/airspeed-kt</property>
              <value>40</value>
            </less-than>
          </condition>
          <command>nasal</command>
          <script>citation.cabin_door.toggle();</script>
        </binding>
      </key>
      <key n="2">
	<name>Ctrl-b</name>
	<desc>toggle speed brakes</desc>
	<binding>
	  <command>property-toggle</command>
	  <property>controls/flight/speedbrake-switch</property>
	</binding>
      </key>
      <key n="67">
        <name>C</name>
        <binding>
          <command>property-toggle</command>
          <property>controls/flight/auto-coordination</property>
        </binding>
      </key>
      <key n="106">
        <name>j</name>
        <desc>Retract spoilers</desc>
        <binding>
          <command>property-assign</command>
          <property>controls/flight/speedbrake-switch</property>
          <value>0</value>
        </binding>
      </key>
      <key n="107">
        <name>k</name>
        <desc>Deploy spoilers</desc>
        <binding>
          <command>property-assign</command>
          <property>controls/flight/speedbrake-switch</property>
          <value>1</value>
        </binding>
      </key>
      <key n="108">
	<name>l</name>
	<desc>toggle panel lights</desc>
	<binding>
          <command>property-toggle</command>
          <property>controls/lighting/panel-lights-switch</property>
        </binding>
      </key>
      <key n="109">
